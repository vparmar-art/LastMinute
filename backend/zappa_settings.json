{
    "dev": {
        "app_function": "main.wsgi.application",
        "django_settings": "main.settings",
        "aws_region": "us-east-1",
        "exclude": [
            "boto3",
            "dateutil",
            "botocore",
            "s3transfer",
            "concurrent"
        ],
        "include": ["db.sqlite3"],
        "profile_name": null,
        "project_name": "main",
        "runtime": "python3.12",
        "s3_bucket": "zappa-deployments-last-minute",
        "environment_variables": {
            "DJANGO_SUPERUSER_USERNAME": "admin",
            "DJANGO_SUPERUSER_EMAIL": "admin@example.com",
            "DJANGO_SUPERUSER_PASSWORD": "adminpassword"
        },
        "serve_static": true,
        "postbuild_script": "python manage.py collectstatic --noinput",
        "post_package": "USE_S3_STATICFILES=1 python manage.py collectstatic --noinput"
    }
}