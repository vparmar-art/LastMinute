{
  "family": "lastminute-backend",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::054037119505:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::054037119505:role/ecsTaskRole",
  "containerDefinitions": [
    {
      "name": "django-app",
      "image": "054037119505.dkr.ecr.us-east-1.amazonaws.com/lastminute-backend:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8000,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "DEBUG",
          "value": "False"
        },
        {
          "name": "ALLOWED_HOSTS",
          "value": "your-domain.com,*.your-domain.com"
        },
        {
          "name": "DB_NAME",
          "value": "postgres"
        },
        {
          "name": "DB_USER",
          "value": "vikash"
        },
        {
          "name": "DB_HOST",
          "value": "last-minute-dev.cm96escgy66l.us-east-1.rds.amazonaws.com"
        },
        {
          "name": "DB_PORT",
          "value": "5432"
        },
        {
          "name": "AWS_MEDIA_BUCKET_NAME",
          "value": "lastminute-media-root"
        },
        {
          "name": "AWS_STORAGE_BUCKET_NAME",
          "value": "zappa-deployments-last-minute"
        },
        {
          "name": "AWS_S3_REGION_NAME",
          "value": "us-east-1"
        },
        {
          "name": "AWS_SNS_ARN",
          "value": "arn:aws:sns:us-east-1:054037119505:app/GCM/notify-driver"
        },
        {
          "name": "AWS_DEFAULT_REGION",
          "value": "us-east-1"
        },
        {
          "name": "DJANGO_SECRET_KEY",
          "value": "django-insecure-k3g7t0l_c9r3#2#i62lbiw)=g%kn4+%a8(yi8p&!+n9lyn&%&n"
        },
        {
          "name": "DB_PASSWORD",
          "value": "vikashparmar"
        },
        {
          "name": "AWS_ACCESS_KEY_ID",
          "value": "AKIAQZFG5AII5U73LK6Y"
        },
        {
          "name": "AWS_SECRET_ACCESS_KEY",
          "value": "whSgnkpjus++V2CxZPmXfploPbz2BUWTngjnuvgX"
        }
      ],
      "secrets": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/lastminute-backend",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "healthCheck": {
        "command": [
          "CMD-SHELL",
          "curl -f http://localhost:8000/health/ || exit 1"
        ],
        "interval": 30,
        "timeout": 5,
        "retries": 3,
        "startPeriod": 60
      }
    }
  ]
}